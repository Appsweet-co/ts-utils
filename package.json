{
  "name": "@appsweet-co/ts-utils",
  "version": "0.10.0",
  "description": "Utility functions for TypeScript",
  "main": "index.js",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint": "7.32.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.24.2",
    "typedoc": "0.22.4",
    "typescript": "4.4.3"
  },
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc",
    "clean": "rm -rf *.js *.js.map **/*.js **/*.js.map ./docs",
    "docs": "open ./docs/index.html",
    "gen": "./bin/gen.sh",
    "prebuild": "eslint **/*.ts && npm run build:docs",
    "readme": "npx @appnest/readme generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Appsweet-co/utils.git"
  },
  "author": "Appsweet.co (http://appsweet.co)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Appsweet-co/ts-utils/issues"
  },
  "homepage": "https://appsweet-co.github.io/ts-utils/",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
