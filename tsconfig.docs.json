{
  "extends": "./tsconfig.json",
  "include": [
    "**/*.ts"
  ],
  "typedocOptions": {
    "entryPoints": [
      "./lib"
    ],
    "entryPointStrategy": "expand",
    "excludeExternals": true,
    "excludeInternal": true,
    "excludePrivate": true,
    "gitRemote": "github",
    "includeVersion": true,
    "out": "docs"
  }
}
